    <div class='body'>
      <div class='container'>

        <p id="notice"></p>



<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= render "devise/shared/error_messages", resource: resource %>

        <div class="field">
        <%= f.label :name %><br />
        <%= f.text_field :name, autofocus: true, id:"inputName" %>
        </div>

        <div class="field">
        <%= f.label :Email %><br />
        <%= f.email_field :email, id:"inputEmail" %>
        </div>

        <div class="field">
        <%= f.label :Password %><em> (6 characters minimum)</em><br />
        <%= f.password_field :password, autocomplete: "off", id:"inputPassword" %>
        </div>

        <div class="field">
        <%= f.label :Password %><em> confirmation</em><br />
        <%= f.password_field :password_confirmation, autocomplete: "off", id:"inputConfirmPassword" %>
        </div>

        <div class="actions">
        <%= f.submit('Sign up',class: 'btn btn-success') %>
        </div>
    <% end %>

<%= render "devise/shared/links" %>


     </div>
    </div>

    <footer>
  <div class='container'>
    <div class='row'>
        <div class='col-xs-3 col-xs-offset-9'>
            <p>CopyRight Infratop.inc</p>
        </div>
    </div>
  </div>
</footer>